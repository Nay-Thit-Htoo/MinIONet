<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
	<GeneratePackageOnBuild>True</GeneratePackageOnBuild>
	<IncludeReferencedProjectsInPack>true</IncludeReferencedProjectsInPack>
	<Title>MinIO + .Net</Title>
	<PackageId>MinIO.NET</PackageId>
	<Version>1.0.2</Version>
	<Authors>Nay Thit Htoo</Authors>	
	<PackageLicenseExpression>MIT</PackageLicenseExpression>
	<RepositoryUrl>https://github.com/Nay-Thit-Htoo/MinIONet</RepositoryUrl>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="AutoMapper" Version="13.0.1" />
    <PackageReference Include="Minio" Version="6.0.3" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\MinIONet.Domain\MinIONet.Domain.csproj" />
  </ItemGroup>

</Project>
